{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "inline-json-import"
  ],
  "env": {
    "production": {
      "plugins": [
        ["transform-remove-console", {
          "exclude": [ "error", "warn"]
        }]
      ]
    }
  }
}
