{
  "name": "react-gis",
  "version": "16.0.2",
  "description": "REACT GIS",
  "main": "./build/src/components/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run build:clean && mkdir build && npm run build:babel && npm run build:less && npm run build:locales",
    "build:clean": "rm -rf build",
    "build:babel": "cd build && mkdir src && babel ../src --out-dir ./src",
    "build:less": "cd build && mkdir css && npm run build:less:main && npm run build:less:dark && npm run build:less:purple",
    "build:less:main": "cd build && node ../node_modules/less/bin/lessc ../less/react-gis.less css/react-gis.css",
    "build:less:dark": "cd build && node ../node_modules/less/bin/lessc ../less/react-gis-dark.less css/react-gis-dark.css",
    "build:less:purple": "cd build && node ../node_modules/less/bin/lessc ../less/react-gis-purple.less css/react-gis-purple.css",
    "build:locales": "cp -r locales/ build/locales/",
    "lint": "node ./node_modules/eslint/bin/eslint.js \"src/**/*.js\" > lint.log",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js --fix \"src/**/*.js\" > lint.log",
    "doc": "jsdoc -r src -R README.md -p -c jsdoc.json -d docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "gis": "git+ssh://git@git.gorilla-technology.com:scrd01/gis.git#master",
    "i18next": "^11.3.2",
    "leaflet": "1.3.1",
    "lodash": "^4.17.10",
    "loglevel": "^1.6.1",
    "loglevel-prefix-persist": "^1.0.3",
    "moment": "^2.22.1",
    "object-path-immutable": "^1.0.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-timebar": "git+ssh://git@git.gorilla-technology.com:scrd01/react-timebar.git#upgrade-react-v16",
    "react-ui": "git+ssh://git@git.gorilla-technology.com:scrd01/react-ui.git#upgrade-react-v16"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1"
  }
}
