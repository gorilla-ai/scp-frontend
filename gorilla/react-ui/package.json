{
  "name": "react-ui",
  "version": "16.0.3",
  "description": "REACT UI library",
  "main": "./build/src/components/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run build:clean && mkdir build && npm run build:babel && npm run build:less",
    "build:clean": "rm -rf build",
    "build:babel": "cd build && mkdir src && babel ../src --out-dir ./src",
    "build:less": "cd build && mkdir css && npm run build:less:main && npm run build:less:dark && npm run build:less:purple && npm run build:less:green",
    "build:less:main": "cd build && node ../node_modules/less/bin/lessc ../less/react-ui.less css/react-ui.css",
    "build:less:dark": "cd build && node ../node_modules/less/bin/lessc ../less/react-ui-dark.less css/react-ui-dark.css",
    "build:less:purple": "cd build && node ../node_modules/less/bin/lessc ../less/react-ui-purple.less css/react-ui-purple.css",
    "build:less:green": "cd build && node ../node_modules/less/bin/lessc ../less/react-ui-green.less css/react-ui-green.css",
    "lint": "node ./node_modules/eslint/bin/eslint.js \"src/**/*.js\" > lint.log",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js --fix \"src/**/*.js\" > lint.log",
    "doc": "jsdoc -r src -R README.md -p -c jsdoc.json -d docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "flatpickr": "^4.4.4",
    "font-gorilla": "git+ssh://git@git.gorilla-technology.com:gorilla/font-gorilla.git#semver:^1.0.0",
    "html2canvas": "^1.0.0-alpha.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "loglevel": "^1.6.1",
    "loglevel-prefix-persist": "^1.0.3",
    "moment": "^2.22.1",
    "object-path-immutable": "^1.0.1",
    "prop-types": "^15.6.1",
    "purecss": "^1.0.0",
    "query-string": "^5.1.1",
    "react": "^16.3.1",
    "react-addons-create-fragment": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.3.1",
    "react-draggable": "^3.0.5",
    "underscore.string": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "less": "^2.7.2"
  }
}
